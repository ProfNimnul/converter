Выделение номера счета из строки


import string
a = '"ПН2424З";;"328168          29021504981ПАТ "МТБ БАНК"";;;;;"21650966ПАТ "МТБ БАНК"";;;;;;"";;;"166.91";"комунальн_ послуги о/р2160100102   КЛИМЕНКО АЛЕКСАНДРМИХАЙЛОВИЧ ПАРКОВАЯ 10 КВ 102";;;;;;'
pos = a.find("о/р")
print (pos)
if pos >=0  :
    print (a[pos+3:pos+13])